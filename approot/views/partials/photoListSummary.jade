div#photosListSummary.page-header
	h2 Photo Summary List
div#photosListSummaryResults
	- if(photosList.length === 0)
		p
			small Zero Results
	- else
		ul.media-grid
			- each photo, pic in photosList
				li
					a(href="/photos/" + photo.title.toFolderNameSafe() + "/" + photo.id + "/")
						img.thumbnail(src='http://farm' + photo.farm + '.static.flickr.com/' + photo.server + '/' + photo.id + '_' + photo.secret + '_s.jpg', alt=photo.title)
